  <Scheme id="1950-1959" name="1950s" type="race_points">
    <Era startYear="1950" endYear="1959"/>
    <Notes>Standard World Championship points for 1950-1959. Fastest lap point is separate.</Notes>

    <PointsTable>
      <Place position="1" points="8"/>
      <Place position="2" points="6"/>
      <Place position="3" points="4"/>
      <Place position="4" points="3"/>
